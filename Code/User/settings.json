            "before": [
                "<leader>",
                "c"
            ],
            "commands": [
                "outline.collapse"
            ]
        },
        {
            "before": [
                "J"
            ],
            "after": [
                "g",
                "T"
            ]
        },
        {
            "before": [
                "K"
            ],
            "after": [
                "g",
                "t"
            ]
        },
        {
            "before": [
                ";"
            ],
            "after": [
                ":"
            ]
        }
    ],
