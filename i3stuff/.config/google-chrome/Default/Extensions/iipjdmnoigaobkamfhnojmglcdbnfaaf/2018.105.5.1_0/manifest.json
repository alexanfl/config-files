{
   "background": {
      "scripts": [ "js/constants.js", "js/ls.js", "js/gacode.js", "js/workhorse.js", "js/bgscript.js" ]
   },
   "browser_action": {
      "default_icon": {
         "16": "img/cf16.png",
         "24": "img/cf24.png",
         "32": "img/cf32.png"
      },
      "default_popup": "popup.html",
      "default_title": "__MSG_browserButtonTitle__"
   },
   "commands": {
      "_execute_browser_action": {
         "suggested_key": {
            "default": "Alt+X"
         }
      },
      "duplicate": {
         "description": "Duplicate tab",
         "suggested_key": {
            "default": "Alt+Shift+D"
         }
      },
      "moveToLast": {
         "description": "Move tab to end of window",
         "suggested_key": {
            "default": "Ctrl+Shift+X"
         }
      },
      "toggleState": {
         "description": "Switch Clutter Free off/on"
      },
      "whitelist": {
         "description": "Add page to whitelist"
      },
      "whitelist_domain": {
         "description": "Add domain to whitelist",
         "suggested_key": {
            "default": "Ctrl+Shift+L"
         }
      }
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "css/cf.css" ],
      "js": [ "js/ext/jquery-2.1.1.min.js", "js/constants.js", "js/ls.js", "js/cf.js" ],
      "matches": [ "\u003Call_urls>" ]
   } ],
   "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
   "default_locale": "en",
   "description": "__MSG_appDescription__",
   "icons": {
      "128": "img/cf128.png",
      "16": "img/cf16.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "49",
   "name": "__MSG_appName__",
   "offline_enabled": true,
   "options_page": "options.html",
   "permissions": [ "contextMenus", "notifications", "alarms", "storage", "unlimitedStorage", "tabs", "webRequest", "\u003Call_urls>" ],
   "short_name": "__MSG_shortName__",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2018.105.5.1",
   "version_name": "2018.105.5.1 release",
   "web_accessible_resources": [ "img/*" ]
}
